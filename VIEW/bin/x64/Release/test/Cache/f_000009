          var gradientFill = ctx4.createLinearGradient(0, 0, 0, 450);
                gradientFill.addColorStop(0, 'rgba(3, 169, 244, 0.3)');
                gradientFill.addColorStop(0.5, 'rgba(3, 169, 244, 0.1)');
                gradientFill.addColorStop(1, 'rgba(3, 169, 244, 0)');
         myChart4 = new Chart(ctx4, {
          type: "line",
          data: {
            labels: timeArray4,
            datasets: [
              {
