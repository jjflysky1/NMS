              displayColors: false,
      
              callbacks: {
                title: function (t, d) {
                  const o = d.datasets.map((ds) => ds.data[t[0].index] + " %    ")
          
                  return o.join(', ');
                },
                label: function (t, d) {
                  return d.labels[t.index];
                }
              }
            },
            title: {
                text: "Public Bandwidth",
                display: false
            },
            maintainAspectRatio: true,
            spanGaps: false,
            elements: {
              line: {
                tension: 0.3
              }
            },
            plugins: {
              filler: {
                propagate: false
              }
            },
            scales: {
                xAxes: [
                  {
                  ticks: {
                      autoSkip: true,
                     
            
                  },
                            gridLines: {
                            display: true,
                            drawOnChartArea: false,
                            }
                  }
                ],
                yAxes: [{
                                display: true,
                                stacked: true,
                                ticks: {
                                    min: 0, // minimum value
                                    max: 100 // maximum value
                                }
                       }]
            }
          }
        });

        

        }
        

