                  const o = d.datasets.map((ds) => ds.data[t[0].index] + " Mb/s")
          
                  return o.join(', ');
                },
                label: function (t, d) {
                  return d.labels[t.index];
                }
              }
            },
            title: {
                text: "Public Bandwidth",
                display: false
            },
            maintainAspectRatio: true,
            spanGaps: false,
