    function go(No,category) {
        
        var obj = document.getElementById("HiddenField1");
        if (obj)
            obj.value = No;
          var obj = document.getElementById("HiddenField2");
        if (obj)
            obj.value = category;

         
       __doPostBack('Button2','')
        
    }
